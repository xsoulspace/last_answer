      "args": ["--dart-define-from-file", "configs/envs/dev.json"],
      "flutterMode": "debug"
    },
    {
      "program": "lib/main.dart",
      "name": "prod dev",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define-from-file", "configs/envs/prod.json"],
