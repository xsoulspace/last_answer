{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "program": "lib/main.dart",
      "name": "mobile prod",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define-from-file=configs/envs/prod.json"],
      "flutterMode": "debug"
    },
    {
      "program": "lib/main.dart",
      "name": "mobile local",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define-from-file=configs/envs/local.json"],
      "flutterMode": "debug"
    },
    {
      "program": "lib/main.dart",
      "name": "web prod",
      "request": "launch",
      "type": "dart",
      "args": [
        "--dart-define-from-file=configs/envs/prod.json",
        "--dart-define=STORE=xsoulspaceWebsite",
        "-d",
        "chrome",
        "--web-port=49660"
      ],
      "flutterMode": "debug"
    },
    {
      "program": "lib/main.dart",
      "name": "web local",
      "request": "launch",
      "type": "dart",
      "args": [
        "--dart-define-from-file=configs/envs/local.json",
        "--dart-define=STORE=xsoulspaceWebsite",
        "-d",
        "chrome",
        "--web-port=49660"
      ],
      "flutterMode": "debug"
    },
    {
      "program": "lib/main.dart",
      "name": "prod (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": ["--dart-define-from-file=configs/envs/prod.json"]
    },
    {
      "program": "lib/main.dart",
      "name": "prod (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": ["--dart-define-from-file=configs/envs/prod.json"]
    }
  ]
}
